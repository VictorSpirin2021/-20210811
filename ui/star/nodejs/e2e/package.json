{
  "name": "e2e",
  "version": "1.0.0",
  "description": "Cypress end to end tests",
  "main": "",
  "scripts": {
    "test": "cypress run",
    "clean_prior_results": "rm -rf mochawesome-report",
    "merge_reports": "mochawesome-merge --reportDir mochawesome-report > mochawesome-report/combined.json",
    "generate_mochawesome_report": "marge mochawesome-report/combined.json",
    "test_mochawesome_combo_report": "npm run clean_prior_results; cypress run --reporter mochawesome --reporter-options overwrite=false; npm run merge_reports; npm run generate_mochawesome_report"
  },
  "author": "",
  "license": "UNLICENSED",
  "private": true,
  "devDependencies": {
    "cypress": "^4.0.0",
    "mocha": "^5.2.0",
    "mochawesome": "^4.1.0",
    "mochawesome-merge": "^2.1.0"
  },
  "dependencies": {
    "@types/mocha": "^5.2.7"
  }
}
