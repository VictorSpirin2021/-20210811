{
    "schemaVersion": "1.0.0",
    "imports": [
        "js/aw-popup.directive",
        "js/aw-column.directive",
        "js/aw-panel-body.directive",
        "js/aw-panel-footer.directive",
        "js/aw-button.directive",
        "js/aw-break.directive"
    ],
    "actions": {
        "logoutAction": {
            "actionType": "JSFunction",
            "method": "terminateSession",
            "inputData": {},
            "events": {
                "success": [ {
                    "name": "awPopup.close",
                    "eventData": {
                        "popupId": "logoutConfirmationPopup"
                    }
                } ]
            },
            "deps": "js/loginService"
        },
        "closePopupWindow": {
            "actionType": "Event",
            "method": "Event",
            "inputData": {
                "events": [ {
                    "name": "awPopup.close",
                    "eventData": {
                        "popupId": "logoutConfirmationPopup"
                    }
                } ]
            }
        }
    },
    "data": {},
    "i18n": {
        "logout": [ "loginModuleMessages" ],
        "logoutConfirmationMessage": [ "loginModuleMessages" ],
        "cancelText": [ "loginModuleMessages" ]
    }
}
