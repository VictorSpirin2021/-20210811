{
    "commands": {
        "viewObjectNavigationPanelCloseCommand": {
            "iconId": "cmdClosePanel",
            "isGroup": false,
            "title": "{{i18n.closeCommandName}}"
        },
        "viewObjectNavigationPanelCommand": {
            "iconId": "cmdListSummaryView",
            "isGroup": false,
            "title": "{{i18n.showViewObjectCommandName}}"
        },
        "stopSimulationCommand": {
            "iconId": "cmdStop",
            "title": "{{i18n.stopSimulationCommandName}}",
            "isGroup": false
        },
        "saveHardcopyCommand": {
            "iconId": "cmdCaptureImage",
            "title": "{{i18n.saveHardcopyCommandName}}",
            "isGroup": false
        },
        "resetCameraCommand": {
            "iconId": "cmdFitToWindow",
            "title": "{{i18n.resetCameraCommandName}}",
            "isGroup": false
        }
    },
    "commandHandlers": {
        "viewObjectNavigationPanelCloseCommandHandler": {
            "id": "viewObjectNavigationPanelCloseCommand",
            "action": "viewObjectNavigationPanelCommandAction",
            "activeWhen": true,
            "visibleWhen": true
        },
        "viewObjectNavigationPanelCommandHandler": {
            "id": "viewObjectNavigationPanelCommand",
            "action": "viewObjectNavigationPanelCommandAction",
            "activeWhen": true,
            "visibleWhen": {
                "condition": "conditions.onMonitorPage"
            }
        },
        "stopSimulationCommandHandler": {
            "id": "stopSimulationCommand",
            "action": "stopSimulationAction",
            "activeWhen": true,
            "visibleWhen": {
                "condition": "conditions.onMonitorPage"
            }
        },
        "saveHardcopyCommandHandler": {
            "id": "saveHardcopyCommand",
            "action": "saveHardcopyAction",
            "activeWhen": true,
            "enableWhen": {
                "condition": "conditions.isViewObjectLoaded"
            },
            "visibleWhen": {
                "condition": "conditions.showSaveHardcopyButton"
            }
        },
        "resetCameraCommandHandler": {
            "id": "resetCameraCommand",
            "action": "resetCameraAction",
            "activeWhen": true,
            "enableWhen": {
                "condition": "conditions.isViewObjectSelected"
            },
            "visibleWhen": {
                "condition": "conditions.onMonitorPage"
            }
        }
    },
    "commandPlacements": {
        "viewObjectNavigationPanelCloseCommandPlacement": {
            "id": "viewObjectNavigationPanelCloseCommand",
            "uiAnchor": "viewObjectNavigationPanelAnchor",
            "priority": 300
        },
        "viewObjectNavigationPanelCommandPlacement": {
            "id": "viewObjectNavigationPanelCommand",
            "uiAnchor": "aw_globalNavigationbar",
            "priority": 1000
        },
        "stopSimulationCommandPlacement": {
            "id": "stopSimulationCommand",
            "uiAnchor": "aw_globalNavigationbar",
            "priority": 1001
        },
        "saveHardcopyCommandPlacement": {
            "id": "saveHardcopyCommand",
            "uiAnchor": "aw_globalNavigationbar",
            "priority": 1002
        },
        "resetCameraCommandPlacement": {
            "id": "resetCameraCommand",
            "uiAnchor": "aw_globalNavigationbar",
            "priority": 1003
        },
        "goBackCommandPlacementFooter": {
            "id": "Awp0GoBack",
            "uiAnchor": "aw_footer",
            "priority": 0
        },
        "viewObjectNavigationPanelCommandPlacementFooter": {
            "id": "viewObjectNavigationPanelCommand",
            "uiAnchor": "aw_footer",
            "priority": 1
        },
        "stopSimulationCommandPlacementFooter": {
            "id": "stopSimulationCommand",
            "uiAnchor": "aw_footer",
            "priority": 1
        },
        "resetCameraCommandPlacementFooter": {
            "id": "resetCameraCommand",
            "uiAnchor": "aw_footer",
            "priority": 1
        }
    },
    "actions": {
        "viewObjectNavigationPanelCommandAction": {
            "actionType": "Event",
            "method": "Event",
            "inputData": {
                "events": [ {
                    "name": "awsidenav.openClose",
                    "eventData": {
                        "id": "monitorView",
                        "commandId": "viewObjectNavigationPanelCommand"
                    }
                } ]
            }
        },
        "stopSimulationAction": {
            "actionType": "JSFunction",
            "method": "showPopup",
            "inputData": {
                "popupId": "stopSimulationPopup",
                "height": "120",
                "width": "380"
            },
            "outputData": {},
            "deps": "js/popupService"
        },
        "saveHardcopyAction": {
            "actionType": "JSFunction",
            "method": "saveHardcopy",
            "inputData": {},
            "outputData": {},
            "deps": "js/monitorService"
        },
        "resetCameraAction": {
            "actionType": "JSFunction",
            "method": "sendResetCamera",
            "inputData": {},
            "outputData": {},
            "deps": "js/monitorService"
        }
    },
    "conditions": {
        "onMonitorPage": {
            "expression": "ctx.onMonitorPage"
        },
        "isViewObjectSelected": {
            "expression": "ctx.isViewObjectSelected"
        },
        "isViewObjectLoaded": {
            "expression": "ctx.isViewObjectSelected && ctx.isViewObjectLoaded"
        },
        "showSaveHardcopyButton": {
            "expression": "ctx.onMonitorPage && !ctx.isMobileDevice"
        }
    },
    "i18n": {
        "closeCommandName": [
            "monitorModuleMessages"
        ],
        "showViewObjectCommandName": [
            "monitorModuleMessages"
        ],
        "stopSimulationCommandName": [
            "monitorModuleMessages"
        ],
        "saveHardcopyCommandName": [
            "monitorModuleMessages"
        ],
        "resetCameraCommandName": [
            "monitorModuleMessages"
        ]
    },
    "messages": {},
    "onEvent": []
}
